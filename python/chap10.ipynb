   "source": [
    "def gaussian(t, t0, scale, w):\n",
    "    return scale * np.exp(-(t-t0)**2/w**2)\n",
    "\n",
    "t = np.linspace(0,5,1000)\n",
    "\n",
    "s = gaussian(t,1,-.5,.2) + gaussian(t,1.5,1,.2) + gaussian(t,0,.2,.2)\n",
    "\n",
    "plt.plot(t, s)"
